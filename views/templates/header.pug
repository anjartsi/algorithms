nav.navbar.navbar-inverse
	div.container-fluid

		div.navbar-header
			button.navbar-toggle.collapsed(type="button" data-toggle="collapse" data-target="#navbar-collapse-1" aria-expanded="false")
				span.sr-only Toggle navigation
				- for (let i = 0; i < 3; i++)
					span.icon-bar
			a.navbar-brand(href="/") Algorithm Visualization

		div.collapse.navbar-collapse#nvbar-collapse-1
			ul.nav.navbar-nav
				-for (let i = 0; i < nav.length; i++) 
					if (!nav[i].dropdown)
						li: a(href=nav[i].link) #{nav[i].text}
					else
						li: a.dropdown-toggle(data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false")
							| #{nav[i].text}
							span.caret
							ul.dropdown-menu
								- for (let i = 0; i < projects.length; i++) 
									li: a(href=projects[i].link) #{projects[i].name}
								